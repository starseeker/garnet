(in-package "COMMON-LISP-USER")

;; check first to see if place is set
(unless (boundp 'Garnet-Gadgets-PathName)
  (error "Load 'Garnet-Loader' first to set Garnet-Gadgets-PathName before loading Gadgets."))

;;; Now load the polyline-creator module
;;;
(unless (get :garnet-modules :polyline-creator)
  (format t "Loading Polyline-Creator...~%")
  (dolist (pair '((:polyline-functions "polyline-functions")
		  (:polyline-creator "polyline-creator")))
    (unless (get :garnet-modules (car pair))
      (load (merge-pathnames (cadr pair) Garnet-Gadgets-PathName)
	    :verbose T)))
  (format t "...Done Polyline-Creator.~%"))

(setf (get :garnet-modules :polyline-creator) t)


